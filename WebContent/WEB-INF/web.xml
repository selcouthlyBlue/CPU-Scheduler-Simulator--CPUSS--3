<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>CPUSS3</display-name>
  <servlet>
    <servlet-name>Upload</servlet-name>
    <servlet-class>cs125.cpuss.gonzalvo_marasigan.controllers.FileUploadServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Scheduler</servlet-name>
    <servlet-class>cs125.cpuss.gonzalvo_marasigan.controllers.SchedulerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Upload</servlet-name>
    <url-pattern>/SelectFile</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Scheduler</servlet-name>
    <url-pattern>/Scheduler</url-pattern>
  </servlet-mapping>
</web-app>